<Window x:Class="FiledResx.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:resc="clr-namespace:FiledResx.Resources"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <x:StaticExtension Member="resc:StringResource.ResourceManager" x:Key="StringResource" />
    </Window.Resources>
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding [TEST], Source={StaticResource StringResource}}"/>
            <TextBlock Text="{Binding [TEST], Source={x:Static resc:StringResource.ResourceManager}}"/>
        </StackPanel>
    </Grid>
</Window>
